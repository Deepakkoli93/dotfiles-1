# account specific information | narendraj9@gmail.com
# imap_pass_s contains password for this account
# imap_pass_s set at init
set imap_pass=$my_imap_pass_p
set smtp_pass=$imap_pass

# IMAP settings
set imap_user="narendraj9@gmail.com"
set folder="imaps://imap.gmail.com/"
set spoolfile="+INBOX"
set record="=[Gmail]/Sent Mail"
set postponed="=[Gmail]/Drafts"

# SMTP settings
set smtp_url="smtp://narendraj9@smtp.gmail.com"
set realname="Narendra Joshi"
set from="narendraj9@gmail.com"
